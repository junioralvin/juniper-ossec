<group name="juniper, ">

<rule id="1985" level="0">
  <decoded_as>mgd</decoded_as>
  <description>Grouping of juniper cmd</description>
  <options>alert_by_email</options>
</rule>

<rule id="19850" level="5">
  <if_sid>1985</if_sid>
  <match>UI_CMDLINE_READ_LINE: User 'user1', </match>
  <description>Comandos executados por user1.</description>
  <options>alert_by_email</options>
</rule>

<rule id="19851" level="5">
  <if_sid>1985</if_sid>
  <match>UI_CMDLINE_READ_LINE: User 'user2', </match>
  <description>Comandos executados por user2.</description>
  <options>alert_by_email</options>
</rule>


<rule id="19852" level="5">
  <if_sid>1985</if_sid>
  <match>UI_CMDLINE_READ_LINE: User 'user', </match>
  <description>Comandos executados por user3.</description>
  <options>alert_by_email</options>
</rule>

<rule id="19853" level="5">
  <if_sid>1985</if_sid>
  <match>UI_DBASE_LOGIN_EVENT: User 'user1' entering configuration mode, </match>
  <description>User1 entering configuration mode.</description>
  <options>alert_by_email</options>
</rule>

<rule id="19854" level="5">
  <if_sid>1985</if_sid>
  <match>UI_DBASE_LOGIN_EVENT: User 'user2' entering configuration mode, </match>
  <description>User2 entering configuration mode.</description>
  <options>alert_by_email</options>
</rule>

<rule id="19855" level="5">
  <if_sid>1985</if_sid>
  <match>UI_DBASE_LOGIN_EVENT: User 'user3' entering configuration mode, </match>
  <description>User3 entering configuration mode.</description>
  <options>alert_by_email</options>
</rule>
</group>
